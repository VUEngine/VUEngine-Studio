  "homepage": "https://github.com/VUEngine/VUEngine-Studio",
  "bugs": {
    "url": "https://github.com/VUEngine/VUEngine-Studio/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VUEngine/VUEngine-Studio.git"
  },
  "engines": {
    "yarn": ">=1.7.0 <2",
    "node": ">=12.14.1"
  },
  "devDependencies": {
    "@theia/cli": "1.40.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-plugin-deprecation": "1.2.1",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-no-unsanitized": "^3.1.5",
    "eslint-plugin-react": "^7.23.2",
    "lerna": "^6.0.1",
    "rimraf": "^2.7.1",
    "ts-node": "^10.0.0",
    "type-fest": "^0.20.2",
    "yargs": "17.2.1"
  },
  "scripts": {
    "clean": "lerna run clean && rimraf node_modules",
    "build": "yarn build:extensions && yarn build:applications",
    "build:dev": "yarn build:extensions && yarn build:applications:dev",
    "build:applications": "yarn build:extensions && lerna run build:prod --concurrency 1",
    "build:applications:dev": "yarn build:extensions && lerna run build --concurrency 1",
    "build:extensions": "lerna run build",
    "download:plugins": "theia download:plugins --rate-limit=15 --parallel=false --ignore-errors",
    "package:applications": "lerna run package --concurrency 1",
    "package:applications:preview": "lerna run package:preview --concurrency 1",
    "package:applications:prod": "lerna run package:prod --concurrency 1",
    "watch": "lerna run --parallel watch",
    "test": "lerna run test",
    "electron": "yarn --cwd applications/electron",
    "browser": "yarn --cwd applications/browser",
    "update:next": "ts-node scripts/update-theia-to-next.ts && lerna run update:next",
    "lint": "eslint --ext js,jsx,ts,tsx scripts && lerna run lint",
    "lint:fix": "eslint --ext js,jsx,ts,tsx scripts --fix && lerna run lint:fix",
    "modules:init": "git submodule update --init",
    "modules:update": "git submodule update --recursive --remote",
    "nls": "yarn nls-extract && yarn nls-localize $1",
    "nls-extract": "theia nls-extract -o extensions/vuengine-studio-extension/i18n/nls.json -r extensions/vuengine-studio-extension -m true",
    "nls-localize": "theia nls-localize CS DE ES FR HU IT JA PL PT-BR PT-PT RU ZH-CN -f extensions/vuengine-studio-extension/i18n/nls.json --free-api true -k $1"
  },
  "theiaPluginsDir": "plugins",
  "theiaPlugins": {
    "vscode.configuration-editing": "https://open-vsx.org/api/vscode/configuration-editing/1.77.0/file/vscode.configuration-editing-1.77.0.vsix",
    "vscode.cpp": "https://open-vsx.org/api/vscode/cpp/1.77.0/file/vscode.cpp-1.77.0.vsix",
    "vscode.git": "https://open-vsx.org/api/vscode/git/1.77.0/file/vscode.git-1.77.0.vsix",
    "vscode.git-base": "https://open-vsx.org/api/vscode/git-base/1.70.2/file/vscode.git-base-1.70.2.vsix",
    "vscode.git-ui": "https://open-vsx.org/api/vscode/git-ui/1.55.2/file/vscode.git-ui-1.55.2.vsix",
    "vscode.image-preview": "https://open-vsx.org/api/vscode/image-preview/1.70.2/file/vscode.image-preview-1.70.2.vsix",
    "vscode.ini": "https://open-vsx.org/api/vscode/ini/1.77.0/file/vscode.ini-1.77.0.vsix",
    "vscode.javascript": "https://open-vsx.org/api/vscode/javascript/1.77.0/file/vscode.javascript-1.77.0.vsix",
    "vscode.json": "https://open-vsx.org/api/vscode/json/1.77.0/file/vscode.json-1.77.0.vsix",
    "vscode.json-language-features": "https://open-vsx.org/api/vscode/json-language-features/1.77.0/file/vscode.json-language-features-1.77.0.vsix",
    "vscode.log": "https://open-vsx.org/api/vscode/log/1.77.0/file/vscode.log-1.77.0.vsix",
    "vscode.make": "https://open-vsx.org/api/vscode/make/1.77.0/file/vscode.make-1.77.0.vsix",
    "vscode.markdown": "https://open-vsx.org/api/vscode/markdown/1.77.0/file/vscode.markdown-1.77.0.vsix",
    "vscode.markdown-language-features": "https://open-vsx.org/api/vscode/markdown-language-features/1.77.0/file/vscode.markdown-language-features-1.77.0.vsix",
    "vscode.media-preview": "https://open-vsx.org/api/vscode/media-preview/1.77.0/file/vscode.media-preview-1.77.0.vsix",
    "vscode.merge-conflict": "https://open-vsx.org/api/vscode/merge-conflict/1.77.0/file/vscode.merge-conflict-1.77.0.vsix",
    "vscode.powershell": "https://open-vsx.org/api/vscode/powershell/1.77.0/file/vscode.powershell-1.77.0.vsix",
    "vscode.search-result": "https://open-vsx.org/api/vscode/search-result/1.77.0/file/vscode.search-result-1.77.0.vsix",
    "vscode.shellscript": "https://open-vsx.org/api/vscode/shellscript/1.77.0/file/vscode.shellscript-1.77.0.vsix",
    "vscode.simple-browser": "https://open-vsx.org/api/vscode/simple-browser/1.77.0/file/vscode.simple-browser-1.77.0.vsix",
    "vscode.xml": "https://open-vsx.org/api/vscode/xml/1.77.0/file/vscode.xml-1.77.0.vsix",
    "vscode.yaml": "https://open-vsx.org/api/vscode/yaml/1.77.0/file/vscode.yaml-1.77.0.vsix"
  },
  "theiaPluginsExcludeIds": [],
  "workspaces": [
    "applications/*",
    "extensions/*"
  ],
  "resolutions": {
    "@types/puppeteer": "^5.4.0",
    "@yarnpkg/parsers": "3.0.0-rc.44",
    "**/msgpackr": "1.8.3",
    "**/multer": "1.4.4-lts.1"
  }
}
